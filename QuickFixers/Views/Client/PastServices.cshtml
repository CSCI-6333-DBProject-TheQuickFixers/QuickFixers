@model IEnumerable<QuickFixers.Data.Models.ScheduledService>

@{
    ViewBag.Title = "Past Services";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Past Services</h2>

<table class="table" style="background-color: white;">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ServiceProviderName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceFee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceAddress)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceProviderName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceFee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceAddress)
            </td>
        </tr>
    }

</table>
